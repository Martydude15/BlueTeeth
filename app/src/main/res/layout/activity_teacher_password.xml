<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/southBlue"
    android:visibility="visible"
    tools:context="com.example.marta.blueteeth.TeacherHomeActivity">

    <EditText
        android:id="@+id/editText2"
        android:layout_width="300dp"
        android:layout_height="35dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="150dp"
        android:background="@drawable/student_connect_text"
        android:hint="Password"
        android:inputType="textPassword"
        android:paddingLeft="15dp"
        android:visibility="visible" />

    <Button
        android:id="@+id/button3"
        android:layout_width="175dp"
        android:layout_height="40dp"
        android:layout_below="@id/editText2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:background="@drawable/login_button"
        android:fontFamily="sans-serif"
        android:linksClickable="false"
        android:onClick="testMethod"
        android:text="Submit"
        android:textColor="@color/southWhite"
        android:textSize="24sp"
        android:visibility="visible" />

</RelativeLayout>
